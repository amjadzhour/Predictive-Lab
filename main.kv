<Label>
    color:(213/255, 234/255, 246/255, 1)
    font_size:20

<Spinner>
    background_color:(164/255, 104/255, 166/255, 1)
<Pag1>
    FloatLayout:
        size:root.width,root.height
        Image:
            source:'pngwing.com (2).png'
            size_hint:(None,None)
            pos_hint:{"top":0.99,"x":0.01}
            
            
        Label:
            text:"Projoct"
            size_hint:(None,None)
            pos_hint:{"top":0.99,"x":0.1}

        Image:
            size_hint:(None,None)
            width:250
            height:250
            pos_hint:{"top":0.92,"x":0.01}
            source:"model1.jpg"   
            allow_stretch: True
            keep_ratio: False
        Button:
            text:"Titanic model"
            on_press:app.root.current="pag2"    
            size_hint:(None,None)
            width:250
            height:60
            pos_hint:{"top":0.82,"x":0.01}

<pag2>
    name:"page2"
    FloatLayout:
        size:root.width,root.height
        Image:
            source:'pngwing.com (2).png'
            size_hint:(None,None)
            pos_hint:{"top":0.99,"x":0.01}
            
            
        Label:
            text:"Titanic model"
            size_hint:(None,None)
            pos_hint:{"top":0.99,"x":0.12}

        Spinner:
            text:"select in class"
            id:sn 
            values:["hight","median","low1"]
            on_text:root.Spinner(sn.text) 
            size_hint:(None,None)
            pos_hint:{"top":0.92,"x":0.03}
            width:200


    
        Label:
            text:"Age"
            size_hint:(None,None) 
            pos_hint:{"center_x":0.5,"top":0.84}  
             
        TextInput:
            multiline:False
            id:age1  
            size_hint:(None,None)
            pos_hint:{"center_x":0.65,"top":0.83} 
            height:40

        Label:
            text:"spouses aboard the Titanic"
            size_hint:(None,None)
            pos_hint:{"top":0.92,"x":0.4}  
        TextInput:
            multiline:False
            id:sibsp1  
            size_hint:(None,None)
            pos_hint:{"top":0.909,"x":0.6}  
            height:40  

        Label:
            text:"Passenger fare"
            size_hint:(None,None)
            pos_hint:{"top":0.87,"x":0.08}  
        TextInput:
            multiline:False
            id:fare1  
            size_hint:(None,None)
            pos_hint:{"top":0.86,"x":0.2}  
            height:40 

      

        Spinner:
            text:"sex"
            id:sn 
            values:["male","female"]
            on_text:root.sex(sn.text) 
            size_hint:(None,None)
            pos_hint:{"top":0.87,"x":0.8}   
            width:200

        Label:
            text:"Passenger Ticket number"
            size_hint:(None,None)
            pos_hint:{"top":0.87,"x":0.4}  
        TextInput:
            multiline:False
            id:ticket1 
            size_hint:(None,None)
            pos_hint:{"top":0.86,"x":0.6}  
            height:40 

            
        Spinner:
            text:"embarked"
            id:sn 
            values:["Queenstown","Cherbourg",'Southampton']
            on_text:root.embarked(sn.text) 
            size_hint:(None,None)
            width:200
            pos_hint:{"top":0.92,"x":0.8}    
        Button:
            text:"predict"
            size_hint:(None,None)
            pos_hint:{"top":0.8,"x":0.4} 
            width:200
            height:200
            on_press:root.pred()

        Label:
            text:""
            id:pr    
